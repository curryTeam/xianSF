{
  "name": "xian_sf",
  "version": "0.1.7",
  "description": "the interface of sf for nodeJs",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "compile": "babel src -d lib",
    "prepublish": "npm run compile",
    "cov": "cross-env NODE_ENV=test nyc --reporter=lcovonly mocha test/*.js --timeout 15000 --recursive --compilers js:babel-register",
    "test": "cross-env NODE_ENV=test mocha test/ --recursive  --compilers js:babel-register"
  },
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "keywords": ["顺丰", "nodeJS", "顺丰下单", "顺丰接口nodejs版"],
  "repository": {
    "type": "git",
    "url": "https://github.com/curryTeam/xianSF.git"
  },
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.18.0",
    "babel-root-import": "^4.1.5",
    "cross-env": "^4.0.0",
    "eslint": "^3.12.2",
    "mocha": "^3.2.0",
    "nyc": "^10.2.0",
    "power-assert": "^1.4.2"
  },
  "dependencies": {
    "moment": "^2.17.1",
    "query-string": "^5.0.0",
    "request": "^2.79.0",
    "xml2js": "^0.4.19"
  }
}
